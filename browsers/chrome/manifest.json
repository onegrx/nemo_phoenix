{
    "name": "Nemo",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Transate foreign words faster and keep track of your seach history.",

    "permissions": [
        "contextMenus",
        "http://localhost:4000/api",
        "storage"
    ],

    "browser_action": {
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["nemo.js", "storage.js"]
    }
}
